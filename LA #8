class Book:
    def __init__(self, title, author):
        self.title = title
        self.author = author

    def display_info(self):
        print(f"Title: {self.title}, Author: {self.author}")

book1 = Book("Harry potter", "JK ROWLING")

book1.display_info()

book2 = Book("Super book", "Sean Gaffney")

book2.display_info()
